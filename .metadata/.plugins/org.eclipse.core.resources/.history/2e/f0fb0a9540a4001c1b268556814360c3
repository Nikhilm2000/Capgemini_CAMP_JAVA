package com.capgemini.day5;
class EmployeeGeneric<A>{
	private A id;

	public EmployeeGeneric(A id) {
		this.id = id;
	}
	public A getId() {
		return id;
	}
	public void setId(A id) {
		this.id = id;
	}
	public void printVal() {
		System.out.println(id);
	}
	
}
public class GenericClass {
	public static void main(String[] args) {
		EmployeeGeneric emp=new EmployeeGeneric<String>("nikhil");
		emp.printVal();
	}
}
