{
	System.out.println("hhhh");
	int num=10;
	float f=(float)0.8;
	double d=22.3;
	System.out.println("num="+num);
	System.out.println("float="+f);
	System.out.println("double="+d);
	System.out.println("int maix value="+ Integer.MAX_VALUE);
	boolean tf;
	tf=true;
	System.out.println("tf="+tf);
	System.out.println("tf="+(!tf));
	char a;
	a='z';
	System.out.println("char="+a);
	
	System.out.println("------------------OPERATOTR--------------------");
	int x=10;
	int y=-2;
	
	int z=x+y;
	System.out.println("z"+z);
	
	z=x-y;
	System.out.println("z"+z);
	
	z=x/y;
	System.out.println("z"+z);
	
	z=x%y;
	System.out.println("z"+z);
	
	System.out.println("x<y"+( x<y));
	System.out.println(x>5? "greaater":"not great");
	
	System.out.println("------------------BIT WISE OPERATOTR--------------------");
	System.out.println(x & y);
	System.out.println(x | y);
	System.out.println(x >> 1);
	System.out.println("x "+ (~x));
	System.out.println("------------------if else--------------------");
     System.out.println("Hello, World!");
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter marks");
     int marks=sc.nextInt();
     if(marks<25)
     System.out.println("F");
      if(marks>25 && marks<45)
     System.out.println("E");
      if(marks>45 && marks<50)
    	     System.out.println("D");
      if(marks>50 && marks<60)
    	     System.out.println("C");
      if(marks>60 && marks<75)
 	     System.out.println("B"); 
      if(marks>75 && marks<90)
    	     System.out.println("A");
      if(marks>90)
 	     System.out.println("S");
      System.out.println("------------------switch case--------------------");
      String str=sc.nextLine();
      System.out.println("enter month-- first 3 digits of a mont ex jan feb\n");
      int days;
      switch(str)
      {
      case "feb":
     	days =28;
     	 break;
      case "apr":
      case "jun":
      case "aug":
      case "oct":
      case "dec":
    	  days =30;
    	  break;
      case "jan":
      case "mar":
      case "may":
      case "july":
      case "sept":
      case "nov":
    	  days =31;
    	  break;
     default: days=-1;
     break;
      }
     System.out.println("month"+str+"has"+days+"days");
     
      
      
}  
