package com.capgemini.day3;
abstract interface Mobile{
	public void Price();
	public void Brand();
}
class Samsung implements Mobile{
	@Override
	public void Price() {
		System.out.println("25000");
		
	}
	@Override
	public void Brand() {
		System.out.println("Samsung S20");
	}
	
}
class Laptop implements Mobile{
	@Override
	public void Price() {
		System.out.println("150000");
		
	}
	@Override
	public void Brand() {
		System.out.println("Apple MacBook Pro");
	}
}
abstract class Student{
	public void sendUSN() {
		System.out.println("1by19cs407");
		
	}

	public void Name() {
		System.out.println("Nikhil");
	}
	abstract public void Marks();
}
class Marks extends njn{
	@Override
	public void Marks() {
		System.out.println("92,45,65,77,88");
	}
}
public class MobileAbstraction {
	public static void main(String[] args) {
		Marks BBK=new Marks();
		BBK.Marks();
		BBK.Name();
		BBK.sendUSN();
		
	}

}
