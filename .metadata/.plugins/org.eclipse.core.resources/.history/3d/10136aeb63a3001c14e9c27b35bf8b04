package com.capgemini.EmployeeManagement;

import java.sql.*;

public class Employee {
	private int id;
	private String name;
	private double Salary;
	private Date doj;

	public Employee(int id, String name, double salary, Date doj) {
		super();
		this.id = id;
		this.name = name;
		Salary = salary;
		this.doj = doj;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getSalary() {
		return Salary;
	}

	public void setSalary(double salary) {
		Salary = salary;
	}

	public Date getDoj() {
		return doj;
	}

	public void setDoj(Date doj) {
		this.doj = doj;
	}
	public String toString() {
		return "[Name"+name+"id"+id+"Salary"+Salary+"Date"+doj+"]";
	}
}