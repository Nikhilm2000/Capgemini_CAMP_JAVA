package com.capgemini.classassignment;
class A{
	String ab;
	public void printA(){
		System.out.println("This is Parent Class A");
	}
	
}
class B extends A{
	public void printB() {
		System.out.println("This is Class B-child class");
	}
}
public class SingleInheritence {
public static void main(String[] args) {
	B obj=new B();
	obj.printB();
}
}
